{
  "Prettier Config": {
    "prefix": "prettierconfig",
    "body": [
      "{",
      "  \"printWidth\": 80,",
      "  \"tabWidth\": 2,",
      "  \"useTabs\": false,",
      "  \"semi\": false,",
      "  \"singleQuote\": true,",
      "  \"trailingComma\": \"all\",",
      "  \"arrowParens\": \"always\",",
      "}"
    ]
  },
  "PostCSS Tailwind": {
    "prefix": "postcsstailwind",
    "body": [
      "{",
      "  \"plugins\": {",
      "    \"@tailwindcss/postcss\": {}",
      "  }",
      "}"
    ]
  }
}
